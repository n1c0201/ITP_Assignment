print("Hello World")
time_wasted = "*"
print(time_wasted)
count = 5
number = 5


i = 0

for i in range (5):
    print(time_wasted * (i+1))


for i in range (5 ,0 , -1):
    print(time_wasted * i)

space = " "
for i in range (count):
    print(space*(count - (i+1)) + time_wasted*(i+1))


for i in range (number):
    print(space * (i) + time_wasted*(number - (i)))

